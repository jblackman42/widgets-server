{
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "fs": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "morgan": "^1.10.0",
    "mssql": "^11.0.1",
    "nodemon": "^3.1.7",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "dev": "nodemon server.js",
    "build-widgets": "npx webpack --config ./config/webpack.config.widgets.js",
    "build-components": "npx webpack --config ./config/webpack.config.components.js",
    "build-bundles": "npm run build-widgets && npm run build-components",
    "build-styles": "npx tailwindcss -i ./client/styles/input.css -o ./dist/styles/widgets.css --config ./config/tailwind.config.js",
    "generate-metadata": "node ./config/generateMetadata.js",
    "build": "npm run build-bundles && npm run build-styles && npm run generate-metadata"
  },
  "name": "custom-widgets",
  "version": "1.0.0",
  "main": "server.js",
  "keywords": [],
